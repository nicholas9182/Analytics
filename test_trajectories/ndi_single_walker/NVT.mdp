include                  = -I../more.lib
define                   = 

; RUN CONTROL PARAMETERS
integrator               = md
tinit                    = 0
dt                       = 0.002
nsteps                   = 100000
init-step                = 0
simulation-part          = 1
comm_mode                = linear
nstcomm                  = 100
comm-grps                = 

; LANGEVIN DYNAMICS OPTIONS
bd-fric                  = 0
ld-seed                  = -1

; ENERGY MINIMIZATION OPTIONS
emtol                    = 10
emstep                   = 0.01
niter                    = 20
fcstep                   = 0
nstcgsteep               = 1000
nbfgscorr                = 10

; TEST PARTICLE INSERTION OPTIONS
rtpi                     = 0.05

; OUTPUT CONTROL OPTIONS
nstxout                  = 1000
nstvout                  = 0
nstfout                  = 0
nstlog                   = 1000
nstcalcenergy            = 1000
nstenergy                = 1000
nstxout-compressed       = 100
compressed-x-precision   = 100
compressed-x-grps        = non-water 
energygrps               = 

; NEIGHBORSEARCHING PARAMETERS
cutoff-scheme            = Verlet
nstlist                  = 10
ns_type                  = grid
pbc                      = xyz
periodic-molecules       = no
verlet-buffer-tolerance  = 0.005
rlist                    = 1.0
rlistlong                = -1
nstcalclr                = -1

; OPTIONS FOR ELECTROSTATICS AND VDW
coulombtype              = PME
coulomb-modifier         = Potential-shift-Verlet
rcoulomb-switch          = 0
rcoulomb                 = 1.0
epsilon_r                = 1
epsilon-rf               = 0
vdwtype                  = PME
vdw-modifier             = Potential-shift-Verlet
rvdw-switch              = 0
rvdw                     = 1.0
DispCorr                 = No
table-extension          = 10
energygrp-table          = 
fourierspacing           = 0.12
fourier_nx               = 0
fourier_ny               = 0
fourier_nz               = 0
pme_order                = 4
ewald_rtol               = 1e-05
ewald-rtol-lj            = 0.001
lj-pme-comb-rule         = Geometric
ewald_geometry           = 3d
epsilon_surface          = 0

; GENERALIZED BORN ELECTROSTATICS
gb-algorithm             = Still
nstgbradii               = 1
rgbradii                 = 1
gb-epsilon-solvent       = 80
gb-saltconc              = 0
gb-obc-alpha             = 1
gb-obc-beta              = 0.8
gb-obc-gamma             = 4.85
gb-dielectric-offset     = 0.009
sa-algorithm             = Ace-approximation
sa-surface-tension       = -1

; OPTIONS FOR WEAK COUPLING ALGORITHMS
tcoupl                   = v-rescale
nsttcouple               = -1
nh-chain-length          = 10
print-nose-hoover-chain-variables = no
tc_grps                  = system
tau_t                    = 0.1
ref_t                    = 298
pcoupl                   = no
pcoupltype               = Isotropic
nstpcouple               = -1
tau-p                    = 1
compressibility          = 
ref-p                    = 
refcoord-scaling         = No

; GENERATE VELOCITIES FOR STARTUP RUN
gen_vel                  = yes
gen_temp                 = 300
gen_seed                 = -1

; OPTIONS FOR BONDS    
constraints              = h-bonds
constraint_algorithm     = LINCS
continuation             = no
Shake-SOR                = no
shake-tol                = 0.0001
lincs_order              = 4
lincs_iter               = 1
lincs_warnangle          = 30
morse                    = no

